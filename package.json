{
  "name": "tyro-native-starter",
  "main": "./main.js",
  "private": true,
  "scripts": {
    "start": "expo start ",
    "start:local": "yarn start --localhost",
    "eject": "expo eject",
    "storybook": "adb reverse tcp:7007 tcp:7007 && storybook start -p 7007 --skip-packager",
    "prestorybook": "rnstl",
    "dev": "concurrently npm:start:local npm:storybook"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src/components"
      ],
      "pattern": "**/*.story.tsx",
      "outputFile": "./storybook/storyLoader.ts"
    }
  },
  "dependencies": {
    "expo": "^30.0.1",
    "react": "16.3.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-30.0.0.tar.gz",
    "styled-components": "^4.0.3",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "@storybook/addon-actions": "^3.4.11",
    "@storybook/addon-knobs": "^3.4.11",
    "@storybook/addon-links": "^3.4.11",
    "@storybook/addon-notes": "^3.4.11",
    "@storybook/addon-storyshots": "^3.4.11",
    "@storybook/addons": "^3.4.11",
    "@storybook/react-native": "^3.4.11",
    "@types/expo": "^30.0.0",
    "@types/expo__vector-icons": "^6.2.3",
    "@types/react": "^16.4.18",
    "@types/react-native": "^0.57.7",
    "@types/styled-components": "^4.0.3",
    "babel": "^6.23.0",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-runtime": "^6.26.0",
    "enzyme": "^3.7.0",
    "jest": "^23.6.0",
    "prop-types": "^15.6.2",
    "react-dom": "16.3.1",
    "react-native-mock": "^0.3.1",
    "react-native-storybook-loader": "^1.8.0",
    "react-native-typescript-transformer": "^1.2.10",
    "storybook-addon-figma": "^0.1.0",
    "typescript": "^3.1.6"
  }
}
